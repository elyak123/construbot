{
    "family": "construbot",
    "containerDefinitions": [
        {
            "name": "constrbot-django",
            "image": "construbot_django:stagging",
            "cpu": 300,
            "memory": 500,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "command": [
                "/gunicorn.sh"
            ],
            "essential": true
        }
    ]
}